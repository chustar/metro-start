{
	"name": "Metro Start",
	"version": "4.2.1",
	"description": "A start screen based on the Windows Metro UI",
	"chrome_url_overrides": {
		"newtab": "start.html"
	},
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"128": "icon128.png"
	},
	"permissions": [
		"http://metro-start.appspot.com/*",
		"http://www.google.com/ig/api",
		"management",
		"bookmarks"
	]
}
