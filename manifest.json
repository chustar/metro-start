{
	"name": "Metro Start",
	"version": "6.1",
	"manifest_version": 2,
	"description": "A start screen based on the Windows Metro UI",
	"chrome_url_overrides": {
		"newtab": "start.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"128": "icons/icon128.png"
	},
	"web_accessible_resources": [
  		"bundle.map"
	],
	"permissions": [
		"http://metro-start.appspot.com/*",
		"http://query.yahooapis.com/*",
		"management",
		"bookmarks",
		"storage"
	]
}
